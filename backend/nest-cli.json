{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gitmoa/src",
  "monorepo": true,
  "root": "apps/gitmoa",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gitmoa/tsconfig.app.json",
    "deleteOutDir": true
  },
  "generateOptions": {
    "spec": false
  },
  "projects": {
    "gitmoa": {
      "type": "application",
      "root": "apps/gitmoa",
      "entryFile": "main",
      "sourceRoot": "apps/gitmoa/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gitmoa/tsconfig.app.json"
      }
    },
    "sl-user-sync": {
      "type": "application",
      "root": "apps/sl-user-sync",
      "entryFile": "main",
      "sourceRoot": "apps/sl-user-sync/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sl-user-sync/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    }
  }
}